          },
          "query": "import \"date\"\r\n\r\nfrom(bucket: \"${Bucket}\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"upsd\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"load_percent\")\r\n  |> keep(columns:[\"_time\", \"_value\"])\r\n  |> map(\r\n      fn: (r) => ({\r\n        r with _value:\r\n          if date.month(t: r._time) >= 6 and date.month(t: r._time) <= 9 then \r\n            (((float(v: r._value) * 4.5) / 1000.0) / 60.0) * ${summer_electricity_price}\r\n          else\r\n            (((float(v: r._value) * 4.5) / 1000.0) / 60.0) * ${winter_electricity_price}\r\n      })\r\n  )\r\n  |> aggregateWindow(every: v.windowPeriod, fn: last, createEmpty: false)\r\n  |> yield(name: \"last\")",
          "refId": "A"
